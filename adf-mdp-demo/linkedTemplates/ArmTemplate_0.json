			"properties": {
				"description": "connect to ADLS Gen 2 via key vault using storage key secret",
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('ls_ADLSGen2_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('ls_ADLSGen2_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
