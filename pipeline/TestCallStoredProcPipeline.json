{
	"name": "TestCallStoredProcPipeline",
	"properties": {
		"description": "Tests calling the stored procedure pipeline via another pipeline",
		"activities": [
			{
				"name": "StoredProcedurePipeline",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "ETL_pl_REST_StoredProcedure_Execute",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"Status": {
							"value": "@pipeline().parameters.status",
							"type": "Expression"
						},
						"MasterProcessNumber": {
							"value": "@pipeline().parameters.processNumber",
							"type": "Expression"
						},
						"RunType": "'N'"
					}
				}
			}
		],
		"parameters": {
			"status": {
				"type": "int",
				"defaultValue": 1
			},
			"processNumber": {
				"type": "int",
				"defaultValue": 3
			}
		},
		"folder": {
			"name": "REST/Testing"
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}