{
	"name": "ETL_pl_REST_StoredProcedure_Execute",
	"properties": {
		"description": "Uses a HTTP connector to call and lookup the resulting json from the master control stored procedure (pxbimstctl) ",
		"activities": [
			{
				"name": "LookUpHTTP",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "POST",
							"requestBody": {
								"value": "@{concat('{', '\"status\": ', string(pipeline().parameters.Status), ', ', '\"processNumber\": ', string(pipeline().parameters.MasterProcessNumber), ', ', '\"runType\": ', pipeline().parameters.RunType, '}')}",
								"type": "Expression"
							},
							"requestTimeout": ""
						}
					},
					"dataset": {
						"referenceName": "DB2StoredProcedureHTTPJson",
						"type": "DatasetReference"
					}
				}
			}
		],
		"parameters": {
			"Status": {
				"type": "int"
			},
			"MasterProcessNumber": {
				"type": "int"
			},
			"RunType": {
				"type": "string",
				"defaultValue": "'N'"
			}
		},
		"folder": {
			"name": "REST  Testing/FinalVersion"
		},
		"annotations": []
	}
}